		  />
		</targetConfig>
		<targetConfig targets="lightning__FlowScreen,">
			<property
			name="title"
			type="String" 
			label="Title"
			description="Appears at the top of the component"
			default="Einstein OCR Viewer"
		  />
		</targetConfig>
	</targetConfigs>
	
</LightningComponentBundle>